<!DOCTYPE html>
<html>
<head>
    <title> Account - View All</title>
</head>
    <body>
        <h1>Account</h1>

        <table border = "1">
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>E-Mail</th>
                <th>Action</th>
            </tr>

            <% for(var i = 0; i < account.length; i++) {%>
            <tr>
                <td><%= account[i].first_name %></td>
                <td><%= account[i].last_name %></td>
                <td><%= account[i].email %></td>
                <td><a href="/account/edit/?account_id=<%= account[i].account_id %>">
                    Edit</a></td>
            </tr>
            <% } %>
        </table>
    </body>
</html>